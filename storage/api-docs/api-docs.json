                "operationId": "7bf41a2f5d0072edfb27584457c7b88e",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "prop_type",
                                    "unit_type",
                                    "unit_no",
                                    "floor",
                                    "suite_area",
                                    "furnished",
                                    "unit_view",
                                    "price",
                                    "building_id",
                                    "status"
                                ],
                                "properties": {
                                    "prop_type": {
                                        "type": "string",
                                        "example": "Residential"
                                    },
                                    "unit_type": {
                                        "type": "string",
                                        "example": "Apartment"
                                    },
                                    "unit_no": {
                                        "type": "string",
                                        "example": "A101"
                                    },
                                    "floor": {
                                        "type": "string",
                                        "example": "1"
                                    },
                                    "parking": {
                                        "type": "string",
                                        "example": "Covered"
                                    },
                                    "pool_jacuzzi": {
                                        "type": "string",
                                        "example": "None"
                                    },
                                    "suite_area": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 120.5
                                    },
                                    "balcony_area": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 15.75
                                    },
                                    "furnished": {
                                        "type": "boolean",
                                        "example": true
