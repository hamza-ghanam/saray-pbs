                                    "unit_view": {
                                        "type": "string",
                                        "example": "City View"
                                    },
                                    "price": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 350000
                                    },
                                    "building_id": {
                                        "type": "integer",
                                        "example": 5
                                    },
                                    "status": {
                                        "type": "string",
                                        "enum": [
                                            "Pending",
                                            "Available",
                                            "Pre-Booked",
                                            "Booked",
                                            "Sold",
                                            "Pre-Hold",
                                            "Hold",
                                            "Cancelled"
                                        ],
                                        "example": "Pending"
                                    },
                                    "floor_plan": {
                                        "description": "Optional floor plan file (pdf, jpg, jpeg, png)",
                                        "type": "string",
                                        "format": "binary"
                                    }
